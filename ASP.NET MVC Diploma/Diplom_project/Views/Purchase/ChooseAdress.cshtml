@model Diplom_project.ViewModels.AdressSelectViewModel
@{
    ViewBag.Title = "ChooseAdress";
}

<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>

@using (Html.BeginForm()) {
    
           <div class="editor-label">
                Виберіть адресу, за якою буде доставлено товар
            </div>
            <div class="editor-field">
                @Html.DropDownListFor(x => x.SelectedItemId,Model.Items)
            </div>
    
    <input type="submit" value="Виконати замовлення" />
}
