@model List<Diplom_project.Gemsinprod>
<div id="album-details">
@if (Model.Count != 0)
{
     foreach (var item in Model)
        {
         <p>
                @item.gem.nameGem :
         </p>
               <p>
                @item.amount шт. @item.gem.weightGem карат., @item.gem.infoGem , @item.gem.formGem 
            </p>
       
        }
}
else
{
      <p>Немає дорогоцінного каміння </p>
}
</div>

